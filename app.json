{
  "expo": {
    "name": "demapps",
    "slug": "demapps",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cricket.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/cricket.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4AA56"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cricket.png",
        "backgroundColor": "#F4AA56"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.gandhiraja.demapps"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6dabacd6-bd21-4103-8eb8-d31039f7e053"
      }
    }
  }
}
